    "description": "A Node.js application that subscribes to a topic on a Confluent Cloud Kafka cluster, sends a sample message, and consumes it.",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts"
    },
    "engines": {
        "node": ">=18.0.0 <23.0.0"
    },
    "dependencies": {
        "@confluentinc/kafka-javascript": "^1.0.0",
        "@types/axios": "^0.9.36",
        "@types/opossum": "^8.1.8",
        "axios": "^1.8.3",
        "opossum": "^8.4.0"
    },
    "devDependencies": {
        "@types/dotenv": "^6.1.1",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "ts-node": "^10.9.2"
    }
}
